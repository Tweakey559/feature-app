{
  "client": {
    "buildDir": "dist",
    "ghUser": "inpercima",
    "installDependencies": true,
    "language": "en",
    "modules": {
      "enabled": true,
      "features": {
        "defaultRoute": "feature",
        "name": "features"
      },
      "notFound": {
        "enabled": false,
        "name": ""
      },
      "routing": true
    },
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "http://feature-app.inpercima.net/",
      "repository": "https://github.com/inpercima/feature-app/"
    },
    "prefix": "fa",
    "theme": "indigo-pink",
    "useGoogleFonts": true,
    "useYarn": true
  },
  "general": {
    "author": "Marcel Jänicke",
    "description": "Administrate and manage features on an account like ig_* on Instagram.",
    "modRewriteIndex": true,
    "name": "feature-app",
    "title": "feature-app",
    "useDocker": false,
    "useMITLicense": true,
    "useMock": true,
    "useSecurity": true
  },
  "server": {
    "backend": "php",
    "javaKt": {
      "management": "",
      "packagePath": ""
    },
    "php": {
      "modRewritePhpExtension": true,
      "serverAsApi": true
    }
  }
}
